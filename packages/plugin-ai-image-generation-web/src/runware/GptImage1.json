{
  "openapi": "3.0.0",
  "info": {
    "title": "GPT Image 1 API",
    "version": "1.0.0"
  },
  "components": {
    "schemas": {
      "GptImage1Input": {
        "type": "object",
        "properties": {
          "prompt": {
            "type": "string",
            "title": "Prompt",
            "x-imgly-builder": {
              "component": "TextArea"
            }
          },
          "size": {
            "type": "string",
            "title": "Format",
            "enum": ["1024x1024", "1536x1024", "1024x1536"],
            "default": "1024x1024",
            "x-imgly-enum-labels": {
              "1024x1024": "Square",
              "1536x1024": "Landscape",
              "1024x1536": "Portrait"
            },
            "x-imgly-enum-icons": {
              "1024x1024": "@imgly/plugin/formats/ratio1by1",
              "1536x1024": "@imgly/plugin/formats/ratio4by3",
              "1024x1536": "@imgly/plugin/formats/ratio3by4"
            }
          },
          "background": {
            "type": "string",
            "title": "Background",
            "enum": ["auto", "transparent", "opaque"],
            "default": "auto",
            "x-imgly-enum-labels": {
              "auto": "Auto",
              "transparent": "Transparent",
              "opaque": "Opaque"
            }
          }
        },
        "required": ["prompt"],
        "x-fal-order-properties": ["prompt", "size", "background"]
      }
    }
  }
}
