{
  "openapi": "3.0.0",
  "info": {
    "title": "Seedream 4.0 API",
    "version": "1.0.0"
  },
  "components": {
    "schemas": {
      "Seedream4Input": {
        "type": "object",
        "properties": {
          "prompt": {
            "type": "string",
            "title": "Prompt",
            "minLength": 2,
            "maxLength": 3000,
            "x-imgly-builder": {
              "component": "TextArea"
            }
          },
          "image_size": {
            "type": "string",
            "title": "Format",
            "enum": [
              "square",
              "square_hd",
              "portrait_4_3",
              "portrait_3_2",
              "portrait_16_9",
              "landscape_4_3",
              "landscape_3_2",
              "landscape_16_9",
              "landscape_21_9"
            ],
            "default": "square_hd",
            "x-imgly-enum-labels": {
              "square": "Square",
              "square_hd": "Square HD",
              "portrait_4_3": "Portrait 4:3",
              "portrait_3_2": "Portrait 3:2",
              "portrait_16_9": "Portrait 16:9",
              "landscape_4_3": "Landscape 4:3",
              "landscape_3_2": "Landscape 3:2",
              "landscape_16_9": "Landscape 16:9",
              "landscape_21_9": "Ultrawide"
            }
          },
          "image_resolution": {
            "type": "string",
            "title": "Resolution",
            "enum": ["1k", "2k", "4k"],
            "default": "1k",
            "x-imgly-enum-labels": {
              "1k": "1K (~1MP)",
              "2k": "2K (~4MP)",
              "4k": "4K (~16MP)"
            }
          }
        },
        "required": ["prompt"],
        "x-fal-order-properties": ["prompt", "image_size", "image_resolution"]
      }
    }
  }
}
