{
  "openapi": "3.0.0",
  "info": {
    "title": "GPT Image 1 Edit API",
    "version": "1.0.0"
  },
  "components": {
    "schemas": {
      "GptImage1EditInput": {
        "type": "object",
        "x-fal-order-properties": ["image_url", "prompt", "size"],
        "properties": {
          "image_url": {
            "type": "string",
            "format": "uri",
            "title": "Source Image"
          },
          "prompt": {
            "type": "string",
            "title": "Prompt",
            "x-imgly-builder": {
              "component": "TextArea"
            }
          },
          "size": {
            "type": "string",
            "title": "Format",
            "enum": ["1024x1024", "1536x1024", "1024x1536"],
            "default": "1024x1024"
          }
        },
        "required": ["image_url", "prompt"]
      }
    }
  }
}
