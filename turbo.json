{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "globalEnv": [
    "CESDK_LICENSE",
    "FAL_AI_PROXY_URL",
    "ANTHROPIC_PROXY_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev:wait"]
    },
    "dev:types": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "dev:wait": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "check:all": {
      "outputs": []
    },
    "check:types": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "check:lint": {
      "outputs": []
    },
    "check:pretty": {
      "outputs": []
    }
  }
}
